{
  "filters": {},
  "rules": {
    "preset-ja-spacing": true,
    "preset-ja-technical-writing": {
      "ja-no-mixed-period": {
        "periodMark": "．"
      },
      "sentence-length": {
        "max": 150,
        "ja-no-mixed-period": {
          "periodMark": "．"
        }
      },
      "ja-space-between-half-and-full-width": {
        "space": "always",
        "exceptPunctuation": true
      },
      "max-ten": {
        "touten": "，",
        "kuten": "．"
      }
    },
    "ja-no-mixed-period": {
      "periodMark": "．"
    },
    "prh": {
      "rulePaths": [
        "./prh.yml"
      ]
    }
  },
  "plugins": [
    "latex2e"
  ]
}